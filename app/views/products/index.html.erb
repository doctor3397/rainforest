<h1>All Products</h1>
<%= link_to "New Product",  new_product_path %>

<% @products.each do |product| %>
  <h2>Name: <%= product.name %></h2>
  <p>Description: <%= product.description %></p>
  <p>Price: <%= product.price_in_cents %></p>
  <%= link_to product.name,  product_path(product)%>
<% end %>
